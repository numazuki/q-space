<% if users.any?%>
  <ul>
    <% users.each do |user|%>
      <li>
        <img>
        <div>
          <div>
            <%= user.name%>
          </div>
          <div>
            <p><%= link_to 'Prof',user_path(user)%></p>
          </div>
        </div>
      </li>
    <%end%>
  </ul>
  <%=paginate users%>
<%end%>